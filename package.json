{
  "name": "react-chrome-extension-boilerplate",
  "version": "0.9.0",
  "description": "Boilerplate for Chrome extension React.js project",
  "scripts": {
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint app chrome test scripts webpack/*.js",
    "lint-fix": "eslint --fix app chrome test scripts webpack/*.js",
    "test-e2e": "cross-env NODE_ENV=test jest --config=./jest-e2e.config.js test/e2e",
    "test": "cross-env NODE_ENV=test jest --config=./jest-test.config.js ./test/app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhen0409/react-chrome-extension-boilerplate.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "author": "Jhen <developer@jhen.me>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.3.19",
    "chromedriver": "^2.33.2",
    "cross-env": "^3.1.3",
    "crx": "^3.2.1",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "fsevents": "^1.2.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "jest-environment-enzyme": "^6.0.0",
    "jest-enzyme": "^6.0.0",
    "jsdom": "^9.2.1",
    "minimist": "^1.2.0",
    "postcss-loader": "^1.2.1",
    "pug-cli": "^1.0.0-alpha6",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.3.2",
    "request": "^2.86.0",
    "rimraf": "^2.4.3",
    "selenium-webdriver": "^2.47.0",
    "shelljs": "^0.7.0",
    "stringstream": "0.0.6",
    "style-loader": "^0.13.1",
    "webpack": "^3.10.0",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-httpolyglot-server": "^0.3.0"
  },
  "dependencies": {
    "autoprefixer": "^7.2.5",
    "bluebird": "^3.3.4",
    "classnames": "^2.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dock": "^0.2.3",
    "react-dom": "^16.3.2",
    "react-redux": "^4.3.0",
    "redux": "^3.2.1",
    "redux-thunk": "^2.0.1"
